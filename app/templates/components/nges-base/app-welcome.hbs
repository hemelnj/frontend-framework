<nav class="navbar navbar-expand-md bg-dark navbar-dark">


  <div class="">
    {{!--#link-to 'welcome.index' class="navbar-brand" }} <div style="font-weight: bold">  {{appName}} </div> {{/link-to--}}
    {{#link-to 'welcome.index' class="" }}
      {{nges-base/app-logo-viewer}}
    {{/link-to}}
  </div>


  <button class="navbar-toggler" type="button" data-toggle="collapse" data-target="#collapsibleNavbar">
    <span class="navbar-toggler-icon"></span>
  </button>
  <div class="collapse navbar-collapse" id="collapsibleNavbar">


    <ul class="navbar-nav  mr-auto">

      {{#each serviceList as |service|}}

        {{log service.displayName}}
        <li class="nav-item nav-link">
          {{#link-to 'welcome.application-loader.panel-loader'
                     service.applicationRouteName
                     service.routeName
                     class="nav-link"
          }} {{t service.displayName}} {{/link-to}}
        </li>

      {{/each}}
    </ul>


    <div class="menu-right">
      {{nges-base/app-application-popup
        userName = userEmail
        signOut = (action 'signOut')
        navigateToUpdatePassword = (action 'navigateToUpdatePassword')
        changeLocaleENG=(action 'changeLocaleENG')
        changeLocaleBNG=(action 'changeLocaleBNG')

      }}
    </div>
  </div>
</nav>

<br>

<div class="container">

  {{
  nges-base/app-greetings
    appName=appName
    changeLocaleENG=(action 'changeLocaleENG')
    changeLocaleBNG=(action 'changeLocaleBNG')
  }}

</div>


