{{!nges-core/json tableData}}

<div class="card">

  <div class="card-body">

    <h1 class="layout-header-title">{{serviceActionableRoute.create.label}}</h1>
    <hr class="header-label-line">

    {{#if viewCreateButton}}
    {{!log serviceActionableRoute.create.routePath}}
    {{!nges-core/nges-link
      route = 'welcome.application-loader.panel-loader.module-loader.service-holder-loader.menu-template-loader.submenu-template-loader.submenu-detail-template-loader'
      routePath = serviceActionableRoute.create.routePath
      label =  serviceActionableRoute.create.label
    }}

      {{!nges-core/nges-link-to
        routePath = serviceActionableRoute.create.routePath
        label =  serviceActionableRoute.create.label
      }}
    {{/if}}

    <br>

    {{#if serviceActionableRoute.export.buttonVisible}}
      <div class="nges-button nges-button-success" {{action
        "exportToPDF" serviceActionableRoute.export.object}}>{{serviceActionableRoute.export.buttonName}}</div>
    {{/if}}

    <br>

    {{nges-elements/nges-tab-table

      activeTabInfo = activeTabInfo
      stateList = stateList
      defaultFunction = defaultFunction
      defaultLocation = defaultLocation
      stateActions = stateActions
      actionAccess = actionAccess


      tableData = tableData
      serviceActionableRoute = serviceActionableRoute

      onSelectedTabCallBack = (action 'onSelectedTabCallBack')
      onSelectedSingleStateCallBack = (action 'onSelectedSingleStateCallBack')
      onSelectedMultipleStateCallBack = (action 'onSelectedMultipleStateCallBack')
    }}

  </div>
</div>