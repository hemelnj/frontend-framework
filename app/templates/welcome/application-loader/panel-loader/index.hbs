{{!nges-core/json model.appPanelInformation}}



{{#if (eq model.appPanelInformation.appPanelCode 'operation') }}

  <div class="card">
    <div class="card-body">
      <h2 class="layout-header-title">Dashboard</h2>
      <hr class="header-label-line">
      <div class="">
        <div class="row">
          <div class="col-md-7">
            {{nges-base/d3-bar-rnd}}
          </div>
          <div class="col-md-5">
            {{nges-base/d3-pi-chart-rnd}}
          </div>
        </div>
      </div>

    </div>
  </div>


{{/if}}

{{#if (eq model.appPanelInformation.appPanelCode 'admin-panel') }}

  <div class="card">
    <div class="card-body">
      <h2 class="layout-header-title">Dashboard</h2>
      <hr class="header-label-line">

      <button class="nges-button nges-button-success" {{action  'changeLocaleENG'}}> ENG</button>
      <button class="nges-button nges-button-success" {{action  'changeLocaleBNG'}}> BNG</button>

      <br>

      <div class="">
        <div class="row">
          <div class="col-md-5">
            {{nges-base/d3-pi-chart-rnd}}
          </div>
          <div class="col-md-7">
            {{nges-base/d3-bar-rnd}}
          </div>

        </div>
      </div>


    </div>
  </div>

{{/if}}






