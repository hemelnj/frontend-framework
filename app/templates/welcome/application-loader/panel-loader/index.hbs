{{!nges-core/json model.appPanelInformation}}



{{#if (eq model.appPanelInformation.appPanelCode 'operation') }}

  <div class="card">
    <div class="card-body">
      <h2 class="layout-header-title">{{t "Dashboard"}}</h2>
      <hr class="header-label-line">

      <div class="">
        <div class="row">
          <div class="col-md-6" style="border: 1px solid #bebebe; padding: 5px;">
            {{nges-base/d3-bar-rnd}}
          </div>
          <div class="col-md-6" style="border: 1px solid #bebebe; padding: 5px;">
            {{nges-base/d3-animated-pi-chart-rnd}}
          </div>
        </div>

        <div class="row">
          <div class="col-md-6" style="border: 1px solid #bebebe; padding: 5px;">
            {{nges-base/d3-horizontal-bar-rnd}}
          </div>
          <div class="col-md-6" style="border: 1px solid #bebebe; padding: 5px;">
            {{nges-base/d3-pi-chart-rnd}}
          </div>
        </div>
      </div>

    </div>
  </div>


{{/if}}

